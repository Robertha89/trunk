/proc/vox(var/input as text)
	set category = "Special Verbs"
	set name = "Vox"
	set desc = "HL Vox"
	//if(!src.holder)
	//	src << "Only administrators may use this command."
	//	return
	//if(!input)
	//	input = input(usr, "Please enter anything you want. Anything. Serious.", "What?", "")
	if(!input)
		return

	log_admin("[key_name(usr)] has created a vox announcement: [input]")
	message_admins("[key_name_admin(usr)] has created a vox announcement", 1)

	var/list/tokens = dd_text2list(input, " ")
	for(var/t in tokens)
		t = lowertext(t)
		if(!voxsounds[t])
			continue
		for(var/client/C)
			spawn(0)
				C << sound(voxsounds[t], wait = 1, channel = 5)

/proc/autovox(var/message = "", var/chan = 5)
	var/list/tokens = dd_text2list(message, " ")
	for(var/t in tokens)
		t = lowertext(t)
		if(!voxsounds[t])
			continue
		for(var/client/C)
			spawn(0)
				C << sound(voxsounds[t], wait = 1, channel = chan)

/proc/dd_text2list(text, separator, var/list/withinList)
	var/textlength = length(text)
	var/separatorlength = length(separator)
	if(withinList && !withinList.len) withinList = null
	var/list/textList = new()
	var/searchPosition = 1
	var/findPosition = 1
	for(var/failsafe = 50, failsafe > 0, failsafe--)
		findPosition = findtext(text, separator, searchPosition, 0)
		var/buggyText = copytext(text, searchPosition, findPosition)
		if(!withinList || (buggyText in withinList)) textList += "[buggyText]"
		if(!findPosition) return textList
		searchPosition = findPosition + separatorlength
		if(searchPosition > textlength)
			textList += ""
			return textList
	return